#include <stdio.h>
int main() {
 int n;
 scanf("%d", &n);
 int arr[n];
 for (int i = 0; i < n; i++) {
 scanf("%d", &arr[i]);
 }
 int unique = 1;
 for (int i = 0; i < n - 1; i++) {
 if (arr[i] == arr[i + 1]) {
 continue;
 } else {
 unique++;
 }
 }
 printf("There are %d distinct element in the array.", unique);
 return 0;
}
