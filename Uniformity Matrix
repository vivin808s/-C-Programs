#include <stdio.h>
int main() {
 int n;
 scanf("%d", &n);
 
 int a[10][10];
 int ecount = 0, ocount = 0;
 for (int i = 0; i < n; i++) {
 for (int j = 0; j < n; j++) {
 scanf("%d", &a[i][j]);
 }
 }
 for (int i = 0; i < n; i++) {
 for (int j = 0; j < n; j++) {
 if (a[i][j] % 2 == 0) {
 ecount++;
 } else {
 ocount++;
 }
 }
 }
 if (ecount == n * n || ocount == n * n) {
 printf("Yes\n");
 } else {
 printf("No\n");
 }
 return 0;
}
