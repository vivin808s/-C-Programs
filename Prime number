#include<stdio.h>
int prime(int,int);
int main()
{
	int n,p;
	scanf("%d",&n);
	p=prime(n,n/2);
	if(p==1)
      printf("Prime Number");
  else
    printf("Not a Prime Number");
}
int prime(int n,int i)
{
	if(i==1)
	{
		return 1;
	}
	else 
	{
		if(n%i==0)
          return 0;
      	else
          return prime(n,i-1);
	}
}

