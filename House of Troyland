#include <stdio.h>
#include <stdlib.h>
int main() {
 int num;
 scanf("%d", &num);
 
 int house_number[num];
 int position[num];
 
 for(int i = 0; i < num; i++) {
 scanf("%d", &house_number[i]);
 scanf("%d", &position[i]);
 }
 int copy_position[num];
 
 for(int i = 0; i < num; i++)
 copy_position[i] = position[i];
 
 int a;
 
 for(int i = 0; i < num; ++i) {
 for(int j = i + 1; j < num; ++j) {
 if(copy_position[i] > copy_position[j]) {
 a = copy_position[i];
 copy_position[i] = copy_position[j];
 copy_position[j] = a;
 }
 }
 }
 int temp;
 int x1, x2;
 int position1, position2;
 int maxi = 0;
 for(int i = 0; i < num - 1; i++) {
 temp = abs(copy_position[i + 1] - copy_position[i]);
 if(temp > maxi) {
 maxi = temp;
 x1 = copy_position[i];
 x2 = copy_position[i + 1];
 }
 }
 for(int i = 0; i < num; i++) {
 if(x1 == position[i])
 position1 = i;
 else if(x2 == position[i])
 position2 = i;
 }
 if(house_number[position1] > house_number[position2]) {
 printf("%d %d", house_number[position2], house_number[position1]);
 } else {
 printf("%d %d", house_number[position1], house_number[position2]);
 }
 return 0;
}
