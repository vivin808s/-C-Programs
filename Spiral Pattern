#include <stdio.h>
int main()
{
  int a[10][10];
  int n;
  scanf("%d",&n);
  for(int i=0;i<n;i++){
    for(int j=0;j<n;j++){
      scanf("%d",&a[i][j]);
    }
  }
  int round = ceil((double)n/2);
  
  for(int i=0;i<round;i++){
    for(int j=i;j<=n-i-1;j++){
      printf("%d ",a[i][j]);
    }
    for(int j=i+1;j<=n-i-1;j++){
      printf("%d ",a[j][n-i-1]);
    }
    for(int j=n-i-2;j>=i;j--){
      printf("%d ",a[n-i-1][j]);
    }
    for(int j=n-i-2;j>i;j--){
      printf("%d ",a[j][i]);
    }
  }
      return 0;
}
