#include <stdio.h>
int main()
{
  int n,m,a,i,sum=0,fact=1;
  scanf("%d",&n);
  m=n;
  while(n!=0)
  {
    a=n%10;
    for(i=1;i<=a;i++)
    {
      fact=i*fact;
      //printf("%d\t%d",n,sum);
    }
    sum=sum+fact;
    n=n/10;
    fact=1;
  }
  if(sum==m)
    printf("Yes");
 
  else
    printf("No");
  // printf("%d",sum);
 }
