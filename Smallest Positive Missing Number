#include<stdio.h>
int main() {
 // Read input from STDIN
 int n;
 scanf("%d", &n);
 // Read array elements
 int a[n];
 for (int i = 0; i < n; i++) {
 scanf("%d", &a[i]);
 }
 // Sort the array (using bubble sort for simplicity)
 for (int i = 0; i < n; i++) {
 for (int j = i + 1; j < n; j++) {
 if (a[i] > a[j]) {
 // Swap elements if they are in the wrong order
 int temp = a[i];
 a[i] = a[j];
 a[j] = temp;
 }
 }
 }
 // Find the smallest missing positive integer
 int missing = 1;
 for (int i = 0; i < n; i++) {
 if (a[i] <= 0) continue;
 if (a[i] == missing) {
 missing++;
 } else if (a[i] > missing) {
 break;
 }
 }
 // Print the result to STDOUT
 printf("%d\n", missing);
 return 0;
}
