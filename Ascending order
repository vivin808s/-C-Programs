#include<stdio.h>
int main()
{
	int n;int i,j,c;
	scanf("%d",&n);
	int a[n];
	//printf("enter the value for the array \n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(j=0;j<n-1;j++)
	{
		for(i=0;i<n-(1+j);i++)
		{
			if(a[i]>a[i+1])
			{
			c=a[i];
			a[i]=a[i+1];
			a[i+1]=c;
			}
		}
		
	}
	printf("The Sorted array is:\n");
	for(i=0;i<n;i++)
	{
		printf("%d\n",a[i]);
	}
}
