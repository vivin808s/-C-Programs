#include <iostream>
#include<math.h>
using namespace std;
int main() 
{
char name[20],grade[10];
  int age;
  float cgpa;

  scanf("%s",&name);
  scanf("%d",&age);
  scanf("%f",&cgpa);
  scanf("%s",&grade);
  printf("Name: %s\n",name);
  printf("Age: %d\n",age);
  printf("CGPA: %.2f\n",floor(cgpa*100)/100);
  printf("Grade: %s",grade);
}
