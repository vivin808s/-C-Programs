#include <stdio.h>
#include <string.h>

void removeCommonCharacters(char str1[], char str2[]) {
    int len1 = strlen(str1);
    int len2 = strlen(str2);

    // Array to mark characters that are present in the first string
    int present[256] = {0};

    // Mark characters from the first string
    for (int i = 0; i < len1; i++) {
        present[str1[i]] = 1;
    }

    // Remove characters from the second string that are present in the first string
    int index = 0;
    for (int i = 0; i < len2; i++) {
        if (!present[str2[i]]) {
            str2[index++] = str2[i];
        }
    }

    // Null-terminate the modified second string
    str2[index] = '\0';
}

int main() {
    char str1[100], str2[100];

    // Input two strings
    scanf("%s %s", str1, str2);

    // Remove common characters and print the modified second string
    removeCommonCharacters(str1, str2);
    printf("%s\n", str2);

    return 0;
}
