#include <stdio.h>
#include <string.h>

int areAnagrams(char str1[], char str2[]) {
    // Calculate the lengths of both strings
    int len1 = strlen(str1);
    int len2 = strlen(str2);

    // If lengths are not equal, strings cannot be anagrams
    if (len1 != len2) {
        return 0; // Not Anagram
    }

    // Arrays to store the frequency of each character
    int frequency1[256] = {0};
    int frequency2[256] = {0};

    // Update character frequencies for the first string
    for (int i = 0; i < len1; i++) {
        frequency1[str1[i]]++;
    }

    // Update character frequencies for the second string
    for (int i = 0; i < len2; i++) {
        frequency2[str2[i]]++;
    }

    // Check if both strings have the same character frequencies
    for (int i = 0; i < 256; i++) {
        if (frequency1[i] != frequency2[i]) {
            return 0; // Not Anagram
        }
    }

    return 1; // Anagram
}

int main() {
    char str1[100], str2[100];

    // Input two strings
    scanf("%s %s", str1, str2);

    // Check if the strings are anagrams and print the result
    if (areAnagrams(str1, str2)) {
        printf("Anagram\n");
    } else {
        printf("Not Anagram\n");
    }

    return 0;
}
