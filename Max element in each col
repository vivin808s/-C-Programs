#include <stdio.h>
int main() {
 // Read the dimensions of the matrix
 int m, n;
 scanf("%d", &m);
 scanf("%d", &n);
 // Declare and read the matrix elements
 int a[m][n];
 for (int i = 0; i < m; i++) {
 for (int j = 0; j < n; j++) {
 scanf("%d", &a[i][j]);
 }
 }
 // Find and print the maximum element in each column
 for (int i = 0; i < n; i++) {
 int max_val = 0;
 for (int j = 0; j < m; j++) {
 if (a[j][i] > max_val) {
 max_val = a[j][i];
 }
 }
 printf("%d\n", max_val);
 }
 return 0;
}
