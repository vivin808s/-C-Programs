#include <stdio.h>
int main()
{
  int size,elements,location,insert;
  scanf("%d",&size);
  int a[size];
  for(int i=0;i<size;i++){
    scanf("%d",&a[i]);
  }
  scanf("%d",&location);
  if(location<=size){
    scanf("%d",&insert);
    printf("Array after insertion is\n");
    int i =0;
    while(i<size){
      if(i==location-1){
        printf("%d\n%d\n",insert,a[i]);
      }
      else{
        printf("%d\n",a[i]);
      }
      i=i+1;
    }
  }
  else{
    printf("Invalid Input");
  }
  
   return 0;
}
